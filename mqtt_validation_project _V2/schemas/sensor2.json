{
    "columns": {
        "sensor_id": {
            "dtype": "str",
            "checks": {
                "equal_to": "sensor2"
            },
            "nullable": false
        },
        "timestamp": {
            "dtype": "str",
            "checks": {
                "str_matches": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}$"
            },
            "nullable": false
        },
        "pressure": {
            "dtype": "float",
            "checks": {
                "greater_than_or_equal_to": 900,
                "less_than_or_equal_to": 1100
            },
            "nullable": false
        },
        "acceleration": {
            "dtype": "float",
            "checks": {
                "greater_than_or_equal_to": -20,
                "less_than_or_equal_to": 20
            },
            "nullable": false
        }
    },
    "strict": true,
    "coerce": true
} 