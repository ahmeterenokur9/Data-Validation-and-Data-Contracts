{
    "columns": {
        "sensor_id": {
            "dtype": "str",
            "checks": {
                "equal_to": "sensor1"
            },
            "nullable": false
        },
        "timestamp": {
            "dtype": "str",
            "checks": {
                "str_matches": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}$"
            },
            "nullable": false
        },
        "temperature": {
            "dtype": "float",
            "checks": {
                "greater_than_or_equal_to": -40,
                "less_than_or_equal_to": 85
            },
            "nullable": false
        },
        "humidity": {
            "dtype": "float",
            "checks": {
                "greater_than_or_equal_to": 0,
                "less_than_or_equal_to": 100
            },
            "nullable": false
        }
    },
    "strict": true,
    "coerce": true
}