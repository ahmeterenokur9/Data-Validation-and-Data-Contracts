{
    "columns": {
        "sensor_id": {
            "dtype": "str",
            "nullable": false,
            "unique": false,
            "checks": {
                "equal_to": "sensor1"
            }
        },
        "timestamp": {
            "dtype": "str",
            "nullable": false,
            "unique": false,
            "checks": {
                "str_matches": "^\\d{4}-\\d{2}-\\d{2} \\d{2}:\\d{2}:\\d{2}$"
            }
        },
        "temperature": {
            "dtype": "float",
            "nullable": false,
            "unique": false,
            "checks": {
                "greater_than_or_equal_to": -40,
                "less_than_or_equal_to": 85
            }
        },
        "humidity": {
            "dtype": "float",
            "nullable": false,
            "unique": false,
            "checks": {
                "greater_than_or_equal_to": "",
                "less_than_or_equal_to": 101
            }
        }
    },
    "strict": true,
    "ordered": false,
    "coerce": false
}